{
  "name": "react-notion-simplify",
  "version": "1.0.13",
  "description": "Fast and accurate React renderer for Notion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:webpack": "webpack --mode production --config webpack.config.cjs",
    "build:tsc": "tsc --emitDeclarationOnly",
    "dev": "webpack serve --mode development --config webpack.config.cjs",
    "watch": "run-p watch:* --continue-on-error",
    "watch:tsc": "tsc --watch --preserveWatchOutput --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "run-p test:*",
    "test:eslint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint '**/*.{ts,tsx}' --fix",
    "example": "webpack serve --mode development --config examples/webpack.config.cjs"
  },
  "peerDependencies": {
    "katex": "^0.16.9",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@matejmazur/react-katex": "^3.1.3",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^3.3.1",
    "lodash.throttle": "^4.1.1",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "react-fast-compare": "^3.2.2",
    "react-image": "^4.1.0",
    "react-modal": "^3.16.1"
  },
  "optionalDependencies": {
    "@fisch0920/medium-zoom": "^1.0.7",
    "react-pdf": "^6.2.2",
    "react-use": "^17.5.0"
  },
  "author": "Jessie <https://github.com/Jessie-jzn>",
  "license": "MIT",
  "keywords": [
    "notion",
    "property",
    "react"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.cjs"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "endOfLine": "lf"
  },
  "yalcSig": "c74b2216414b33e4446e48e7da139502"
}
